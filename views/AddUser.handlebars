{{#section 'head'}}
    <link href="assets/css/form.css" rel="stylesheet">
{{/section}}


<div class="main_container" id="forms_page">
        <div class="row-fluid">
          <ul class="breadcrumb">
            <li><a href="#">Home</a> <span class="divider">/</span></li>
            <li><a href="users.html">Users</a> <span class="divider">/</span></li>
            <li class="active">Add User</li>
          </ul>
          <h2 class="heading">
                Add User
          </h2>
        </div>
        <!-- 登陆信息 -->
        <div class="row-fluid" id="loginInfoBox">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>Login Information</h5>
            </div>
            <div class="widget-body">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                 <div class="control-group">
                    <label class="control-label">Username</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Username">
                    </div>
                  </div>
                 <div class="control-group">
                    <label class="control-label">Password</label>
                    <div class="controls">
                      <input class="span7 tip" type="password" name="Password">
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>  

        <!-- 基本信息 -->
        <div class="row-fluid">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>Name and Address</h5>
            </div>
            <div class="widget-body">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                 <div class="control-group">
                    <label class="control-label">Name</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Name">
                    </div>
                  </div>
                 <div class="control-group">
                    <label class="control-label">Social security Number</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Social_security_Number">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Mailing Address</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Mailing_Address">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">city state Zip Code</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="city_state_Zip_Code">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Telephone</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Telephone">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Age</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Age">
                      <span class="help-inline">If under 18,please list age.</span>
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Email</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Email">
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>  



        <!-- 工作性质 -->
        <div class="row-fluid">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>Job Type</h5>
            </div>
            <div class="widget-body">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                   <div class="control-group">
                    <label style="text-align:left">Days/hours available to work:</label>
                    <div class="controls controls-wdq">
                      <label class="checkbox">
                        <input name="Days_work" type="checkbox" value="1">
                        Mon
                      </label>
                      <label class="checkbox">
                        <input name="Days_work" type="checkbox" value="2">
                        Tues
                      </label>
                      <label class="checkbox">
                        <input name="Days_work" type="checkbox" value="3">
                        Wed
                      </label>
                      <label class="checkbox">
                        <input name="Days_work" type="checkbox" value="4">
                        Thurs
                      </label>
                      <label class="checkbox">
                        <input name="Days_work" type="checkbox" value="5">
                        Fri
                      </label>
                      <label class="checkbox">
                        <input name="Days_work" type="checkbox" value="6">
                        Sat
                      </label>
                      <label class="checkbox">
                        <input name="Days_work" type="checkbox" value="7">
                        Sun
                      </label>
                      <label class="checkbox">
                        <input name="Days_work" type="checkbox" value="0">
                        I have no preference.
                      </label>
                    </div>
                  </div>

                  <div class="control-group">
                    <label class="control-label">I am seeking a:</label>
                    <div class="controls controls-wdq">
                      <label class="radio">
                        <input type="radio" name="Work_nature" value="0" checked>
                        Full-time job
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Work_nature" value="1">
                        Part-time job
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Work_nature" value="2">
                        Full- or Part-time
                      </label>
                    </div>
                  </div>

                  <div class="control-group">
                    <label>How many hours can you work weekly?</label>
                    <div class="controls">
                      <input class="span7 tip" name="Work_hours" type="text">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Can you work nights?</label>
                    <div class="controls controls-wdq">
                      <label class="radio">
                        <input type="radio" name="Work_at_night" value="1" checked>
                        Yes
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Work_at_night" value="0">
                        No
                      </label> 
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Date available to begin</label>
                    <div class="controls">
                      <div class="input-append date span5 datepicker datepicker-basic" data-date="" data-date-format="dd-mm-yyyy">
                          <input class="date-box" size="16" type="text" name="Work_available_date" value="">
                          <span class="add-on"><i class="icon-th"></i></span>
                        </div>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>  


        <!-- 附加信息 -->
        <div class="row-fluid">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>Additional Information</h5>
            </div>
            <div class="widget-body">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                  <div class="control-group">
                    <label>I certify that a U.S. citizen,permanent resident,or a foreign national with authorization to work in the United States.</label>
                    <div class="controls controls-wdq">
                      <label class="radio">
                        <input type="radio" name="Is_Legal_status" value="1" checked>
                        Yes
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Is_Legal_status" value="0">
                        No
                      </label> 
                    </div>
                  </div>
                  <div class="control-group">
                    <label>Have you ever been convicted of,or entered a plea of guilty,no contest,or had a withheld judgment to a felony.</label>
                    <div class="controls controls-wdq">
                      <label class="radio">
                        <input type="radio" name="Have_Criminal_Record" value="1" checked>
                        Yes
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Have_Criminal_Record" value="0">
                        No
                      </label> 
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">If Yes,please explain:</label>
                    <div class="controls">
                      <textarea class="span7" rows="5" style="height:100px;" name="Criminal_Record"></textarea>
                    </div>
                  </div>
                  <div class="control-group">
                    <label>Do you have a driver's license?</label>
                    <div class="controls controls-wdq">
                      <label class="radio">
                        <input type="radio" name="Have_DL" value="1" checked>
                        Yes
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Have_DL" value="0">
                        No
                      </label> 
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Driver's license number</label>
                    <div class="controls">
                      <input class="span7" type="text" name="DL_Number">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Issued in what state?</label>
                    <div class="controls">
                      <input class="span7" type="text" name="DL_Issued_State">
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>

        
        <!-- 高中学历信息 -->
      <div id="highSchoolBox"> 
        <div class="row-fluid">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>Education（High School）</h5>
              <div class="widget-buttons">
                  <a data-title="Add" onclick="addHighSchool()"><i class="icon-plus"></i></a>
              </div>
            </div>
            <div class="widget-body highSchoolItem">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                  <div class="control-group">
                    <label class="control-label">School</label>
                    <div class="controls">
                      <input class="span7" type="text" name="School_Name">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Mailing Address</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="School_Address">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Years Completed</label>
                    <div class="controls">
                      <div class="input-append date span5 datepicker datepicker-basic" data-date="" data-date-format="dd-mm-yyyy">
                          <input size="16" type="text" name="Years_Completed" value="">
                          <span class="add-on"><i class="icon-th"></i></span>
                        </div>
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Major</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Major">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Degree or Diploma</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Degree_Diploma">
                    </div>
                  </div>
                </form>
              </div>
            
            </div>
          </div>
        </div>
      </div>





        <!-- 大学学历信息 -->
      <div id="collegeSchoolBox"> 
        <div class="row-fluid">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>Education（College or Business/Trade School）</h5>
              <div class="widget-buttons">
                  <a data-title="Add" onclick="addCollegeSchool()"><i class="icon-plus"></i></a>
              </div>
            </div>
            <div class="widget-body collegeItem">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                  <div class="control-group">
                    <label class="control-label">School</label>
                    <div class="controls">
                      <input class="span7" type="text" name="School_Name">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Mailing Address</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="School_Address">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Years Completed</label>
                    <div class="controls">
                      <div class="input-append date span5 datepicker datepicker-basic" data-date="" data-date-format="dd-mm-yyyy">
                          <input size="16" type="text" name="Years_Completed" value="">
                          <span class="add-on"><i class="icon-th"></i></span>
                        </div>
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Major</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Major">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Degree or Diploma</label>
                    <div class="controls">
                      <input class="span7 tip" type="text" name="Degree_Diploma">
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>





        <!-- 服役信息 -->
        <div class="row-fluid">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>Military</h5>
            </div>
            <div class="widget-body">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                  <div class="control-group">
                    <label>Have you even been in the Armed Forces?</label>
                    <div class="controls controls-wdq">
                      <label class="radio">
                        <input type="radio" name="Is_Jioned_Army" value="1" checked>
                        Yes
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Is_Jioned_Army" value="0">
                        No
                      </label> 
                    </div>
                  </div>
                  <div class="control-group">
                    <label>Are you now a member of the National Guard?</label>
                    <div class="controls controls-wdq">
                      <label class="radio">
                        <input type="radio" name="Is_Member_NG" value="1" checked>
                        Yes
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Is_Member_NG" value="0">
                        No
                      </label> 
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Specialty</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Military_Specialty">
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>

        
        <!-- 工作经历 -->
      <div id="workBox"> 
        <div class="row-fluid">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>Work Experience</h5>
              <div class="widget-buttons">
                  <a data-title="Add"  onclick="addWork()"><i class="icon-plus"></i></a>
              </div>
            </div>
            <div class="widget-body workItem">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                  <div class="control-group">
                    <label class="control-label">Company</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Company_Name">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Name of last supervisor</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Company_Supervisor">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Hrs/week</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Company_Hours">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Address</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Company_Address">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">City,State,and Zip Code</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Company_ZipCode">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Phone number</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Company_Phone">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Your last job title</label>
                    <div class="controls">
                      <input class="span7" type="text" name="Company_Job_Title">
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Start Date</label>
                    <div class="controls">
                      <div class="input-append date span5 datepicker datepicker-basic" data-date="" data-date-format="dd-mm-yyyy">
                          <input size="16" type="text" name="Company_Start_Date" value="">
                          <span class="add-on"><i class="icon-th"></i></span>
                        </div>
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">End Date</label>
                    <div class="controls">
                      <div class="input-append date span5 datepicker datepicker-basic" data-date="" data-date-format="dd-mm-yyyy">
                          <input size="16" type="text" name="Company_End_Date" value="">
                          <span class="add-on"><i class="icon-th"></i></span>
                        </div>
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Starting Salary</label>
                    <div class="controls">
                      <div class="input-append">
                        <input class="span7" type="text" name="Company_Start_Salary" placeholder="5.000"><span class="add-on">$</span>
                      </div>
                    </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">Final Salary</label>
                    <div class="controls">
                      <div class="input-append">
                        <input class="span7" type="text" name="Company_End_Salary" placeholder="5.000"><span class="add-on">$</span>
                      </div>
                    </div>
                  </div>
                  <div class="control-group">
                    <label>Reason for leaving(be specific)</label>
                    <div class="controls">
                      <textarea class="span7" rows="5" style="height:100px;" name="Company_Reason_Leaving"></textarea>
                    </div>
                  </div>
                  <div class="control-group">
                    <label>List the jobs you held,duties performed,skills used or learned,advancements or promotions while you worked at this company.</label>
                    <div class="controls">
                      <textarea class="span7" rows="5" style="height:100px;" name="Company_Self_Summary"></textarea>
                    </div>
                  </div>
                  <div class="control-group">
                    <label>May we contact this employer?</label>
                    <div class="controls controls-wdq">
                      <label class="radio">
                        <input type="radio" name="Company_Could_Contact" value="1" checked>
                        Yes
                      </label> 
                      <label class="radio">
                        <input type="radio" name="Company_Could_Contact" value="0">
                        No
                      </label> 
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>




        
        <!-- 熟人信息 -->
        <div class="row-fluid">
          <div class="widget widget-padding span12">
            <div class="widget-header">
              <i class="icon-list-alt"></i><h5>References</h5>
            </div>
            <div class="widget-body">
              <div class="widget-forms clearfix">
                <form class="form-horizontal">
                  <div class="control-group">
                    <label style="color:red;">Please include name,phone number,and cirumstances of your acquaintance,Exclude relatives and former employers.</label>
                  </div>
                  <div class="control-group">
                    <label class="control-label">No.1</label>
                      <div class="controls">
                        <textarea class="span7" rows="5" style="height:100px;"></textarea>
                      </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">No.2</label>
                      <div class="controls">
                        <textarea class="span7" rows="5" style="height:100px;"></textarea>
                      </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">No.3</label>
                      <div class="controls">
                        <textarea class="span7" rows="5" style="height:100px;"></textarea>
                      </div>
                  </div>
                  <div class="control-group">
                    <label class="control-label">No.4</label>
                      <div class="controls">
                        <textarea class="span7" rows="5" style="height:100px;"></textarea>
                      </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>


       <!-- 提交按钮 -->
        <div class="row-fluid" style="text-align:center">
           <button type="button" class="btn btn-success" id="form_sublime">submit</button>
        </div>
</div>


{{#section 'script'}}
    <script type="text/javascript" src="assets/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="assets/js/bootstrap-datepicker.js"></script>  
    <script type="text/javascript" src="controller/adduser.js"></script>
{{/section}}
